class UserController < ApplicationController
  def login
  end
  def logout
  end
  def create_student
    @student = Student.new(params[:user])
    if @student.save
      flash[:success] = "Welcome to the SVL!"	    
      redirect_to @student
    else
      @title = "Create New Student"
      render 'create_student'
    end
  end
end
